{
  "manifest_version": 2,
  "name": "Dont Show Me This",
  "description": "Chrome Extension to hide certain content from social networks",
  "version": "1.0",
  "page_action": {
    "default_title": "Dont Show Me This"
  },
  "content_scripts": [
      {
        "matches": ["https://www.facebook.com/*"],
        "js": ["bower_components/jquery/dist/jquery.js", "js/main.js"]
      }
    ],
  "permissions": [
    "storage",
    "activeTab"
  ]
}
